{
  "name": "chapter-spain-web-frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "new:component": "hygen component new",
    "new:page": "hygen page new",
    "eslint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "check-deps": "npx @guidesmiths/license-checker --outputFileName COPYRIGHT --failOn /AGPL*/",
    "postinstall": "npm run check-deps",
    "prepare": "husky install"
  },
  "dependencies": {
    "@stitches/react": "^1.2.8",
    "axios": "^1.4.0",
    "i18next": "^22.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.3.1",
    "react-router-dom": "^6.11.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@guidesmiths/license-checker": "~2.0.1",
    "@playwright/test": "^1.34.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-istanbul": "^0.31.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "hygen": "^6.2.11",
    "jsdom": "^22.1.0",
    "msw": "^1.2.1",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.31.4"
  }
}
